@Article{ent,
AUTHOR = {Rajan, Del and Visser, Matt},
TITLE = {Quantum Blockchain Using Entanglement in Time},
JOURNAL = {Quantum Reports},
VOLUME = {1},
YEAR = {2019},
NUMBER = {1},
PAGES = {3--11},
URL = {https://www.mdpi.com/2624-960X/1/1/2},
ISSN = {2624-960X},
ABSTRACT = {We propose a conceptual design for a quantum blockchain. Our method involves encoding the blockchain into a temporal GHZ (Greenberger&ndash;Horne&ndash;Zeilinger) state of photons that do not simultaneously coexist. It is shown that the entanglement in time, as opposed to an entanglement in space, provides the crucial quantum advantage. All the subcomponents of this system have already been shown to be experimentally realized. Furthermore, our encoding procedure can be interpreted as nonclassically influencing the past.},
DOI = {10.3390/quantum1010002}
}


